<html>
	<head>
		<title>Admin Kasparo</title>
		<link rel="stylesheet" type="text/css" href="/public/bootstrap/bootstrap.css">
		<link rel="stylesheet" type="text/css" href="/public/bootstrap/bootstrap-responsive.css">
		<link rel="stylesheet" type="text/css" href="/public/css/docs.css">
		{% block javascript %}
		{% endblock %}
	</head>
	<body>
		<div class="navbar navbar-inverse navbar-fixed-top">
	      <div class="navbar-inner">
	        <div class="container">
	          <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
	            <span class="icon-bar"></span>
	            <span class="icon-bar"></span>
	            <span class="icon-bar"></span>
	          </button>
	          <a class="brand" href="http://www.kasparo.es">Kasparo</a>
	          <div class="nav-collapse collapse">
	            <ul class="nav">
	              <li{% if nav == "index" %} class="active"{% endif %}>
	                <a href="{{path('admin')}}">Home</a>
	              </li>
	              <li{% if nav == "images" %} class="active"{% endif %}>
	                <a href="{{path('admin_images')}}">Imagenes</a>
	              </li>
	            </ul>
	          </div>
	        </div>
	      </div>
	    </div>
	    <div style="height: 70px;"></div>
	    <div class="container">
	    	{% block section %}
	    	{% endblock %}
	    </div>
	    {% block modal %}
	    {% endblock %}
	</body>
</html>